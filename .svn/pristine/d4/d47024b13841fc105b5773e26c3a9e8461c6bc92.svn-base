.flow{
	width: 100%;
	height: 140px;
	border-bottom: solid 2px #D3D3D3;
	position: relative;
	padding-left: 5%;
}

.img{
	width: 30%;
	float: left;
	padding: 6% 2%;
}

.right{
	width: 65%;
	float: left;
	padding-left: 5%;
	padding-top: 4%;
}
.right div{
	margin: 2% 0;
}
.num div{
	float: left;
	margin-right: 4%;
}

.mui-numbox{
	width:60% !important;
	height: 30px !important;
	border-radius: 20px !important;
}

.btm{
	height: 20px;
	position: absolute;
	bottom: 1%;
}

#home_nav{
	position: fixed;
	bottom: 0%;
}

#defaultTab b{
	color: red;
}
.mui-checkbox input[type=checkbox]:checked:before{
	color: darkgreen !important;
}

