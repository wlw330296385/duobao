/*project*/
#project{
	height: 702px;
	
}
#project .title{
	height: 60px;
	background: url(../img/gsxm.png) no-repeat;
	margin-bottom: 44px;
}
.projectTitle{
	border-bottom: 3px solid #ff8b02;
}
.projectList{
	width: 1060px;
	border: 1px solid #ddddde;
	padding: 12px 0;
	background: #FFFFFF;
	color: #666666;
	overflow: hidden;
	
}
.projectTitle .tt{
	font-size: 16px;
	line-height: 42px;
	border: 1px solid #ddddde;
	padding: 0 24px;
	margin-left: -1px;
	
}
.projectTitle .tt a{
	color: #666666;
}
.projectTitle .tt:nth-of-type(1){
	background: #ff8b02;
	border-color: #ff8b02;
	color: #FFFFFF;
}
.projectTitle .ttr{
	font-size: 14px;
	line-height: 42px;
}
.projectTitle .ttr span{
	padding: 0 15px;
}
.projectTitle .ttr a{
	color: #999999;
	padding: 0 15px;
	position: relative;
}
.projectTitle .ttr a:before{
	content: "";
	display: inline-block;
	background: #e2e1e2;
	width: 1px;
	height: 14px;
	position: absolute;
	left: 0;
	top: 0;
	
}
.projectTabl{
	padding: 30px;
	font-size: 14px;
}

.projectTabl th{
	height: 44px;
	font-weight: normal;
}
.projectTabl tr{
	overflow: hidden;
	border-bottom: 1px dashed #ddddde;
}
.projectTabl .dtitle{
	width: 222px;
	height: 73px;
}
.projectTabl .dtitle span{
	display: inline-block;
	width: 200px;
	height: 20px;
	overflow: hidden;
	background: url(../img/bao.jpg) no-repeat;
	padding-left: 22px;
	white-space: nowrap;
	text-overflow: ellipsis;
	line-height: 20px;
}
.projectTabl .dmomey{
	width: 96px;
	height: 73px;
	text-align: center;
}
.projectTabl .drate{
	width: 122px;
	height: 73px;
	text-align: center;
}
.projectTabl .drate strong{
	color: #a60000;
}
.projectTabl .dmoth{
	width: 90px;
	height: 73px;
	text-align: center;
}
.projectTabl .dway{
	width: 106px;
	height: 73px;
	text-align: center;
}
.projectTabl .dprogress{
	width: 203px;
	height: 73px;
}
.projectTabl .dprogress div{
	width: 120px;
	height: 10px;
	background: #eae9e9;
	float: left;
	margin-left: 25px;
}
.projectTabl .dprogress span{
	display: block;
	height: 10px;
	width: 40%;
	background: #f99f1c;
}
.projectTabl .dprogress strong{
	
	float: left;
	font-weight: normal;
	margin-left: 10px;
	line-height: 10px;
}
.projectTabl .ddetail{
	width: 148px;
	height: 73px;
	text-align: center;
}
.projectTabl .ddetail a{
	display: inline-block;
	line-height: 30px;
	padding: 0 30px;
	background: #ff8b02;
	color: #FFFFFF;
}


