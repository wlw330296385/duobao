
/*reset*/
html{
	overflow-x: hidden;
}
body{
	margin: 0;
	font-family: arial "微软雅黑" "宋体";
}
ul,li,h1,p,h2,h3,h4,input,textarea,table,tr,td,tbody{
	margin: 0;
	padding: 0;
}
ul{
	list-style: none;
}
a{
	text-decoration: none;
}
img,input{
	border: none;
}
input{
	background: none;
}
table{
	border-collapse: collapse;
	border: none;
}
td,th{
	border: none;
}
/*commet*/
.wrap{
	width: 1082px;
	margin: 0 auto;
	/*overflow: hidden;*/
	position: relative;
}
.clearfix{
	zoom: 1;
}
.clearfix:after{
	content: "";
	display: block;
	clear: both;
}
.fl{
	float: left;
}
.fr{
	float: right;
}

/*header*/

#head{
	width: 1082px;
	height: 100px;
	margin: 0;
background: url(../img/logo.png) no-repeat;
	
}
#head .logo{
	height: 80px;
	width: 80px;
	margin-top: 10px;
	
	background-size: contain;
	text-indent: -9999px;
}
#head .nav{
	position: absolute;
	top: 35px;
	left: 340px;
	overflow: hidden;
}
#head .nav .ul{
	
}
#head .nav li{
	float: left;
	border-radius: 5px;
	margin: 0 5px;
}
#head .nav li.active{
	background: #ea5404;
}
#head .nav li.active a{
	color: #FFFFFF;
}
#head .nav li:hover{
	background: #ea5404;
	
}
#head .nav li:hover a{
	color: #FFFFFF;
}
#head .nav li a{
	font-size: 18px;
	color: #555555;
	float: left;
	line-height: 40px;
	padding: 0 16px;
	
	
}
/*banner*/
#banner{
	background: #e6e6e6;
	height: 400px;
}
#banner .bannerShow{
	width: 1082px;
	height: 400px;
	position: relative;
}
#banner .bannerShow li{
	width: 1082px;
	height: 400px;
	position: absolute;
	left: 1082px;
	top: 0;
	opacity: 0;
}
#banner .bannerShow li.show{
	opacity: 1;
	left: 0px;
}
#banner .bannerShow img{
	width: 100%;
	height: 100%;
}
#banner .bannerBtn{
	position: absolute;
	left: 461px;
	bottom: 20px;
	width: 160px;
		
}
#banner .bannerBtn span{
	float: left;
	width: 20px;
	height: 20px;
	border-radius: 10px;
	background: rgba(0,0,0,0.4);
	margin: 0 5px;
	cursor: pointer;
	box-sizing: border-box;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	-o-box-sizing: border-box;
	-ms-box-sizing: border-box;
}
#banner .bannerBtn .active{
	background: rgba(255,255,255,0.5);
	border: 1px solid rgba(0,0,0,0.6);
	
}
/*tagerShow*/
#tagerShow{
	height: 282px;
	padding: 20px 0 80px 0;
}
#tagerShow .title{
	height: 60px;
	background: url(../img/5dys.png) no-repeat;
	margin-bottom: 41px;
}
.car{
	width: 216px;
	text-align: center;
	font-size: 14px;
	line-height: 28px;
	color: #777777;
	float: left;
}
.car img{
	transition: 0.5s;
	-webkit-transition: 0.5s;
	-moz-transition: 0.5s;
	-o-transition: 0.5s;
	-ms-transition: 0.5s;
}
.car:hover img{
	transform: rotate(360deg);
	-webkit-transform: rotate(360deg);
	-moz-transform: rotate(360deg);
	-o-transform: rotate(360deg);
	-ms-transform: rotate(360deg);
}

/*case*/

#case{
	height: 560px;
	background: #F1F1F1;
}
#case .title{
	height: 60px;
	background: url(../img/ywbk.png) no-repeat;
	margin-bottom: 41px;
}
.caseList{
	perspective: 800px;
	-webkit-perspective: 800px;
	-moz-perspective: 800px;
	-o-perspective: 800px;
	-ms-perspective: 800px;
}
.caseList .caseContante{
	width: 383px;
	height: 182px;
	float: left;
	position: relative;
	margin: 10px 0px 20px 84px;
	transform-style: preserve-3d;
	-webkit-transform-style: preserve-3d;
	-moz-transform-style: preserve-3d;
	-o-transform-style: preserve-3d;
	-ms-transform-style: preserve-3d;
}
.caseList .caseContante div{
	width: 383px;
	height: 182px;
	position: relative;
	cursor: pointer;
	transform: rotateX(0deg);
	-webkit-transform: rotateX(0deg);
	-moz-transform: rotateX(0deg);
	-o-transform: rotateX(0deg);
	-ms-transform: rotateX(0deg);
	transform-style: preserve-3d;
	-webkit-transform-style: preserve-3d;
	-moz-transform-style: preserve-3d;
	-o-transform-style: preserve-3d;
	-ms-transform-style: preserve-3d;
	-webkit-transition: 0.5s;
	-moz-transition: 0.5s;
	-o-transition: 0.5s;
	-ms-transition: 0.5s;
	transition: 0.5s;
}
.caseList .caseContante div:hover{
	transform: rotateX(180deg);
	-webkit-transform: rotateX(180deg);
	-moz-transform: rotateX(180deg);
	-o-transform: rotateX(180deg);
	-ms-transform: rotateX(180deg);
}
.caseList .caseContante img{
	display: block;
	position: absolute;
	left: 0;
	top: 0;
	z-index: 2;
	-webkit-backface-visibility: hidden;
	-moz-backface-visibility: hidden;
	-o-backface-visibility: hidden;
	-ms-backface-visibility: hidden;
	
}
.caseList .caseContante p{
	width: 100%;
	height: 100%;
	position: absolute;
	left: 0;
	top: 0;
	font-size: 18px;
	color: #fff;
	line-height: 26px;
	/*white-space: nowrap;
	text-overflow:ellipsis; */
	overflow:hidden; 
	box-sizing: border-box;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	-o-box-sizing: border-box;
	-ms-box-sizing: border-box;
	padding: 10px 10px 0 10px;
	transform: rotateX(-180deg);
	-webkit-transform: rotateX(-180deg);
	-moz-transform: rotateX(-180deg);
	-o-transform: rotateX(-180deg);
	-ms-transform: rotateX(-180deg);
	backface-visibility: hidden;
	-webkit-backface-visibility: hidden;
	-moz-backface-visibility: hidden;
	-o-backface-visibility: hidden;
	-ms-backface-visibility: hidden;
}
.caseList .caseContante p span{
	display: inline-block;
	margin: 10px 0;
}
