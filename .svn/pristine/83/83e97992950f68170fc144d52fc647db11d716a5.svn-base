.contain input{
	border: none !important;
	width: 82% !important;
	margin: 0;
	padding-right: 2%;
}

.contain input::-webkit-input-placeholder{ 
	text-align: right !important; 
	font-size: 0.8em;
	padding: 0;
	}
	
.contain input::-ms-input-placeholder{
	text-align: right !important;
	padding: 0;
	}
.contain ul li{
margin:3% 0 ;
width: 100%;
display:block; /*内联对象需加*/
word-break:keep-all;/* 不换行 */
white-space:nowrap; /* 不换行,但是引起内容可以左右移动 */
width: 100%;
	padding:0 5%;
	background: #fff;
} 

.mui-tab-label{
	background: red; 
	height: 50px; 
	line-height: 50px; 
	font-size: 1.5em !important;
	color: #fff;
	text-align: center;
}
